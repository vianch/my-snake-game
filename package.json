{
  "name": "my-snake-game",
  "version": "1.0.0",
  "description": "snake game in js",
  "main": "src/index.js",
  "repository": "git@github.com:vianch/my-snake-game.git",
  "author": "Victor Chavarro <victor.chavarro@todaytixgroup.com>",
  "license": "MIT",
  "scripts": {
    "build:dev": "NODE_ENV='development' rollup  --config rollup.config.mjs --watch",
    "dev": "smooth-server --config server-config.json",
    "lint:style": "eslint -c .eslintrc.js  --fix src",
    "lint:formatting": "prettier --write src",
    "lint": "npm run lint:style && npm run lint:formatting",
    "init": "rm -rf package-lock.json && rm -rf yarn.lock && rm -rf yarn-error.log && rm -rf node_modules && yarn install"
  },
  "devDependencies": {
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "prettier": "^3.1.1",
    "rollup": "^4.9.2",
    "rollup-plugin-uglify": "^6.0.4",
    "smooth-server": "^1.3.1"
  },
  "resolutions": {
    "serialize-javascript": "6.0.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.2"
  }
}
